<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="NiIdeaService"
                   namespace="NiIdeaService.Dominio"
                   default-lazy="false">
	<class name="Responsable" table="t_Responsable">
		<id name="Codigo" column="codigo">
			<generator class="identity" />
		</id>
		<property name="Nombre" column="nombre" />
		<property name="ApePaterno" column="apepaterno" />
		<property name="ApeMaterno" column="apematerno" />
		<property name="Correo" column="correo" />
		<property name="Telefono" column="telefono" />
		<property name="Edad" column="edad" />
		<property name="Sexo" column="sexo" />
		
	</class>
</hibernate-mapping>

